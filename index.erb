<script>
    // JavaScript code for animating typed text
    document.addEventListener('DOMContentLoaded', function() {
      var typedText = document.getElementById('animated-text');
      var textToType = typedText.innerHTML.trim(); // Get the innerHTML of the paragraph
      typedText.innerHTML = ''; // Clear the existing content

      var typingSpeed = 60; // Adjust the typing speed (in milliseconds)
      var currentIndex = 0;

      function typeNextCharacter() {
        if (currentIndex < textToType.length) {
          typedText.innerHTML += textToType[currentIndex];
          currentIndex++;
          setTimeout(typeNextCharacter, typingSpeed);
        }
      }

      typedText.parentElement.style.visibility = 'visible'; // Show the text container
      setTimeout(function() {
        typedText.parentElement.style.opacity = '1'; // Set opacity to 1 after a slight delay
        typeNextCharacter(); // Start typing animation
      }, 500);
    });
  </script>
  <script>
  // JavaScript code
  
  $(document).ready( function() {
    // Retrieve the carousel container element
    var carouselContainer = document.getElementById('image-carousel');

    // Define an array of image URLs
    var imageUrls = [
      'image1.jpg',
      'image2.jpg',
      'image3.jpg'
    ];

    // Create the carousel HTML structure dynamically
    var carouselHtml = '';
    for (var i = 0; i < imageUrls.length; i++) {
    carouselHtml += '<img src="' + imageUrls[i] + '" alt="image' + (i+1) + '">';
    }

 


    // Set the carousel HTML content
    carouselContainer.innerHTML = carouselHtml;

    // Add carousel functionality using a library or custom JavaScript code
    // Example using Slick carousel library:
    $(carouselContainer).slick({
    prevArrow: '<button type="button" class="slick-prev">   > </button>',
      nextArrow: '<button type="button" class="slick-next">  < </button>'
   });
  });
</script>

<script>
// Retrieve the moving image element
var movingImage = document.getElementById('moving-image');

// Get the container size
var containerHeight = document.querySelector('.container').offsetHeight;

// Calculate the maximum translation distance
var maxTranslateY = containerHeight - movingImage.offsetHeight;

// Update the image position when the user scrolls
function updateImagePosition() {
  // Check if the .container is in the viewport
  var containerRect = document.querySelector('.container').getBoundingClientRect();
  var isInViewport = containerRect.top < window.innerHeight && containerRect.bottom > 0;

  if (isInViewport) {
    // Calculate the scroll percentage within the .container
    var scrollPercentage =  ((window.scrollY - containerRect.top) / containerRect.height);
    scrollPercentage *= 0.5 // Multiply by a smaller factor for slower movement

    // Calculate the new image position
    var newTranslateY = scrollPercentage * maxTranslateY;

    // Update the image position using CSS transforms
    movingImage.style.transform = 'translateY(-' + newTranslateY + 'px)';
  }
}

// Update the image position when the user scrolls
window.addEventListener('scroll', updateImagePosition);
</script>





