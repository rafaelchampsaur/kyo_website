<script>
// Toggle background color and text color of the footer
document.addEventListener('DOMContentLoaded', function() {
  var body = document.body;
  var footer = document.querySelector('footer');
  var footerLinks = document.querySelectorAll('.footer-menu a, .footer-social a');
  var visibles = document.querySelector('.logo');
  var toggleButton = document.getElementById('toggleButton');

  toggleButton.addEventListener('click', function() {
        body.classList.toggle('footer-visible');

  if (body.classList.contains('footer-visible')) {
    footer.style.backgroundColor = 'white';
    footer.style.color = 'black';
    footerLinks.forEach(function(link) {
      link.style.color = 'black';
    });
    visibles.style.display = 'block';
    visibles.style.position = 'fixed';
    visibles.style.top = '20px';
    visibles.style.right= '10px';
    visibles.style.zIndex= '999';
    
   }
  
  else {
    footer.style.backgroundColor = 'black';
    footer.style.color = 'white';
    footerLinks.forEach(function(link) {
      link.style.color = 'white';
    });
    visibles.style.display = 'block';
    visibles.style.position = 'absolute';
   
    }
  });
});
       
 
</script>
